\n  })\n  \n  React.useEffect(() => {\n    dispatch(fetchCoffee(currentFilterCategory, currentSortBy))\n  } , [currentFilterCategory, currentSortBy]);\n\n  const onSelectCategory = React.useCallback((index) => {\n    dispatch(setCategory(index))\n  }, [])\n\n  const onSelectSortBy = React.useCallback((sortName) => {\n    dispatch(setSortBy(sortName))\n  }, [])\n\n  const onClickAddCoffee = React.useCallback((coffeeItem) => {\n    dispatch(addCoffeeToCart(coffeeItem))\n  }, [])\n\n  return (\n    <div className=\"content\">\n      <nav className=\"filters\">\n        <Categories\n          activeCategory={currentFilterCategory}\n          onClickCategory={onSelectCategory}\n          items={categoryNames}\n        />\n        <FiltersSorting\n          activeSortBy={currentSortBy}\n          onClickSortBy={onSelectSortBy}\n          sortingOptions={sortItems}\n        />\n      </nav>\n      <div className=\"cards-wrapper\">\n        {  isLoaded\n            ? coffeeItems.map((item, i) => { return (\n              <CoffeeCard\n                onAddToCart={onClickAddCoffee}\n                key={item.id}\n                addedCount={cartItems[item.id] && cartItems[item.id].items.length}\n                {...item}\n              />)}\n            )\n            : Array(12).fill(0).map((_, index) => <CoffeeLoadingCard key={index} />)\n        }\n      </div>\n    </div>\n  );\n}\n\nexport default Shop;\n","/Users/arsenypomazkov/Desktop/webdev/coffee-guys/src/components/CartItem.jsx",["102"],"/Users/arsenypomazkov/Desktop/webdev/coffee-guys/src/redux/store.js",[],"/Users/arsenypomazkov/Desktop/webdev/coffee-guys/src/redux/reducers/filters.js",[],"/Users/arsenypomazkov/Desktop/webdev/coffee-guys/src/redux/reducers/coffee.js",[],"/Users/arsenypomazkov/Desktop/webdev/coffee-guys/src/redux/reducers/index.js",[],"/Users/arsenypomazkov/Desktop/webdev/coffee-guys/src/redux/actions/coffee.js",[],"/Users/arsenypomazkov/Desktop/webdev/coffee-guys/src/redux/actions/filters.js",[],"